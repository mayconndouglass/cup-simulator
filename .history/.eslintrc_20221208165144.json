{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": 0,
        "no-console": 0,
        "arrow-parens": 0,
        "no-underscore-dangle": 0,
        "lines-between-class-members": 0,
        "quotes": 0,
        "import/extensions": 0,
        "no-unused-expressions": 0,
        "no-param-reassign": 0,
        "implicit-arrow-linebreak": 0
    }
}
